<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="ultimoRoundPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>ObjetosDB.Kitproductos</class>
    <class>ObjetosDB.OrdenDeCompra</class>
    <class>ObjetosDB.CompraProducto</class>
    <class>ObjetosDB.Productos</class>
    <class>ObjetosDB.OrdenDeVenta</class>
    <class>ObjetosDB.VentaProducto</class>
    <class>ObjetosDB.Usuarios</class>
    <class>ObjetosDB.Cliente</class>
    <class>GUI.Productos1</class>
    <class>ObjetosDB.Promociones</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/UltimoRound"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="idp"/>
    </properties>
  </persistence-unit>
</persistence>
